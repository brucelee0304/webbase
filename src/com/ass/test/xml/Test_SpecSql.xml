<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="Test_SpecSql" >
	

<select id="testSelect1"  parameterType="java.util.HashMap" resultType="java.util.HashMap">
	select name1 
	from t_test
	where name1 like '${name1}'
</select>

<select id="testSelect2"  parameterType="java.util.HashMap" resultType="java.util.HashMap">
	select name1 
	from t_test
	where id = '${id}'
</select>






  
  
</mapper>